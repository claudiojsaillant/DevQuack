<nav class="navbar navbar-light bg-light">
    <a class="navbar-brand" href="#">
        <img src="data:image/jpeg;base64," width="60"
            height="60" class="d-inline-block align-top" alt="">
        <div class="web-name">DevQuack</div>
    </a>
    <form class="form-inline my-2 my-lg-0">
        <button type="button" class="btn btn-info">Profile</button>
        <button type="button" class="btn btn-warning">Dashboard</button>
        <button type="button" class="btn btn-danger">Sign Out</button>
    </form>
</nav>

<div class="container">
    <div class="row bio-section">
        <div class="col">
            <div class="user-image">
                <img id="user-image-id"
                    src="https://hips.hearstapps.com/hmg-prod.s3.amazonaws.com/images/avengers-end-game-chris-evans-captain-america-1-1552569157.jpg"
                    alt="image">
            </div>
        </div>
        <div class="col">

            <div id="name-div-bio-section">
                <p>
                    Name: <span>{{user.firstName}} {{user.lastName}}</span>
                </p>
            </div>
            <div id="no-of-post">
                <p>
                    Total Number of Post: <span>{{postsNumber}}</span>
                </p>
            </div>

            <div class="dropdown">
                <a class="btn btn-secondary dropdown-toggle" href="#" role="button" id="dropdownMenuLink"
                    data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    Catagory
                </a>

                <div class="dropdown-menu" aria-labelledby="dropdownMenuLink">
                    <a class="dropdown-item" href="/welcome/1/1">Public</a>
                    <a class="dropdown-item" href="/welcome/1/3">Private</a>
                    <a class="dropdown-item" href="/welcome/1/2">Project</a>
                </div>
            </div>
            <div id="create-a-post"><a href="#" class="btn btn-primary">Create a Post</a></div>

        </div>
    </div>
    <div id="newPostForm">
        <p>
            Title
        </p>
        <input id="user-profile-title"></input>
        <p>
            Content
        </p>
        <input id="user-profile-content">
        </input>
        <p><div id="Submit-a-post"><a href="#" class="btn btn-primary">Submit</a></div></p>
    </div>
    <div class="row post-section">
        <div class="col">
            <div class="card">
                <div class="card-header section-header">
                    Public Posts
                </div>

                <div class="card-body">

                    {{#each publicPosts}}
                    <div class="row post-section-post">
                        <div class="col">
                            <p class="card-text">{{this.title}}
                            </p>
                            <p class="card-text">{{this.content}}
                            </p>
                            <a href="#" class="btn btn-primary">Ratings</a>
                            <a href="#" class="btn btn-primary">Quack</a>
                        </div>
                    </div>
                    {{/each}}
                </div>
            </div>
        </div>


    </div>
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js"
        integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo"
        crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"
        integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1"
        crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"
        integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM"
        crossorigin="anonymous"></script>
    <link rel="stylesheet" href="/styles/profile.css" media="screen" title="no title" charset="utf-8">
    <link href="https://fonts.googleapis.com/css?family=Acme&display=swap" rel="stylesheet">
    <script type="text/javascript">
    $("#newPostForm").hide()
    $("#create-a-post").on("click", function(){
        console.log("You got clicked")
        $("#newPostForm").show();
        $("#create-a-post").hide()
        $("#Submit-a-post").show();
        
        $("#Submit-a-post").on("click",function(){
            $("#Submit-a-post").hide();
            $("#newPostForm").hide();
            console.log($("#user-profile-title").val().trim());
            console.log($("#user-profile-content").val().trim());
            $("#create-a-post").show();
        })
        
    })
     function submitPost(post) {
    $.post("/api/posts", post, function() {
      window.location.href = "/blog";
    });
  }
        $(".dropdown-item").on("click", function () {

            console.log($(this).text());
            $(".btn:first-child").text($(this).text());
        })
        console.log(window.location);
        var pageName = window.location.pathname;
        console.log(pageName);
        var pageType = pageName[pageName.length - 1]
        console.log("pageType", pageType);
        if (pageType === "1") {
            $("#dropdownMenuLink").text("Public")
            $(".section-header").text("Public Posts")
        }
        else if (pageType === "2") {
            $("#dropdownMenuLink").text("Project")
            $(".section-header").text("Project Posts")
        }
        else {
            $("#dropdownMenuLink").text("Private")
            $(".section-header").text("Private Posts")
        }
    </script>